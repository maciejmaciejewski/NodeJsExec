  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NodeJSExec",
      "program": "${workspaceFolder}/NodeJSExec/index.js",
      "outFiles": [
          "${workspaceFolder}/**/*.js"
      ],
      "sourceMaps": true,
      "env": {
        "INPUT_CWD": "./",
        "INPUT_DEPENDENCIES": "{\"hat\": \"0.0.3\"}",
        "INPUT_SCRIPT": "const hat = require('hat'); console.log(`hat = ${hat()}`)",
        "DEBUG":"true"
      },
      "outputCapture": "std"
    }
  ]
}
